{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "blade-ui-kit/blade-heroicons": "^2.2.1",
        "creativeorange/gravatar": "^1.0.23",
        "google/apiclient": "^v2.15.1",
        "guzzlehttp/guzzle": "^7.2",
        "helgesverre/brain": "^0.0.7",
        "helgesverre/milvus": "^0.0.2",
        "laravel/framework": "^10.0",
        "laravel/sanctum": "^3.2",
        "laravel/socialite": "^5.6",
        "laravel/tinker": "^2.8",
        "laravel/vapor-core": "^v2.35",
        "league/flysystem-aws-s3-v3": "^3.22",
        "livewire/livewire": "^3.3",
        "spatie/regex": "^3.1.1",
        "symfony/dom-crawler": "^7.0",
        "usernotnull/tall-toasts": "^v2.0.1"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^v3.9",
        "fakerphp/faker": "^v1.22",
        "laravel/pint": "^v1.13",
        "laravel/sail": "^v1.26",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.1",
        "spatie/laravel-ignition": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "format": [
            "./vendor/bin/pint"
        ],
        "app:clear": [
            "php artisan config:clear",
            "php artisan route:clear",
            "php artisan cache:clear",
            "php artisan view:clear",
            "php artisan event:clear",
            "php artisan optimize:clear",
            "php artisan clear-compiled"
        ],
        "app:cache": [
            "php artisan route:cache",
            "php artisan config:cache",
            "php artisan view:cache",
            "php artisan event:cache"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan vendor:publish --force --tag=livewire:assets --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
